{
  "name": "swagger-validator",
  "publisher": "EmSav",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/msavastano/swagger-validator-vs"
  },
  "displayName": "Swagger Validator",
  "description": "Validates swagger open-api specs",
  "version": "1.1.3",
  "engines": {
    "vscode": "^1.52.0"
  },
  "keywords": [
    "swagger",
    "openapi",
    "api",
    "json",
    "yaml"
  ],
  "galleryBanner": {
    "color": "#4a4a4a",
    "theme": "dark"
  },
  "icon": "static/images/api.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onLanguage:json"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "./node_modules/.bin/vsce package",
    "release": "./node_modules/.bin/standard-version",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3",
    "vsce": "^1.85.0",
    "vscode-test": "^1.5.0",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "swagger-parser": "^10.0.2",
    "yaml": "^1.10.0"
  }
}
